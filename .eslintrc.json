{
    "extends": "@hakatashi",
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "rules": {
        "camelcase": "off",
        "no-console": "off",
        "max-len": "off",
        "react/no-multi-comp": "off",
        "react/forbid-component-props": "off",
        "node/no-unsupported-features": "off",
        "node/no-unsupported-features/es-syntax": "off",
        "node/no-unsupported-features/es-builtins": "off"
    }
}
